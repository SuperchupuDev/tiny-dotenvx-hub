{
  "name": "tiny-dotenvx-hub",
  "version": "0.1.0",
  "description": "A tiny and modern alternative to the deprecated dotenvx-ext-hub",
  "type": "module",
  "scripts": {
    "check": "biome check",
    "format": "biome format --write",
    "lint": "biome lint",
    "lint:fix": "biome lint --fix --unsafe",
    "test": "node --experimental-test-coverage --test"
  },
  "bin": {
    "env-hub": "./src/cli/index.js"
  },
  "files": [
    "src"
  ],
  "author": "Superchupu",
  "license": "BSD-3-Clause",
  "keywords": [
    "dotenv",
    "env"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotenvx/tiny-dotenvx-hub.git"
  },
  "bugs": {
    "url": "https://github.com/dotenvx/tiny-dotenvx-hub/issues"
  },
  "homepage": "https://github.com/SuperchupuDev/tiny-dotenvx-hub#readme",
  "dependencies": {
    "@clack/core": "^0.3.4",
    "commander": "^12.1.0",
    "is-wsl": "^3.1.0",
    "open": "^10.1.0",
    "undici": "^6.19.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "packageManager": "pnpm@9.4.0"
}
